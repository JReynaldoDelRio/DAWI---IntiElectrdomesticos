package com.inti.model;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.JoinColumn;
import jakarta.persistence.ManyToOne;
import jakarta.persistence.Table;
import lombok.Data;

@Entity
@Table (name = "tb_producto")
public class Producto {
	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	private int id_prod;
	private int id_alm;
	private int id_marc;
	private int stock_prod;
	private double precio_prod;
	
	@ManyToOne
	@JoinColumn(name = "id_alm", insertable = false, updatable = false)
	private Almacenamiento objAlm;
	
	@ManyToOne
	@JoinColumn(name = "id_marc", insertable = false, updatable = false)
	private Marca objMarc;

	public Producto(int id_prod, int id_alm, int id_marc, int stock_prod, double precio_prod, Almacenamiento objAlm,
			Marca objMarc) {
		super();
		this.id_prod = id_prod;
		this.id_alm = id_alm;
		this.id_marc = id_marc;
		this.stock_prod = stock_prod;
		this.precio_prod = precio_prod;
		this.objAlm = objAlm;
		this.objMarc = objMarc;
	}

	public Producto() {
		super();
	}

	public int getId_prod() {
		return id_prod;
	}

	public void setId_prod(int id_prod) {
		this.id_prod = id_prod;
	}

	public int getId_alm() {
		return id_alm;
	}

	public void setId_alm(int id_alm) {
		this.id_alm = id_alm;
	}

	public int getId_marc() {
		return id_marc;
	}

	public void setId_marc(int id_marc) {
		this.id_marc = id_marc;
	}

	public int getStock_prod() {
		return stock_prod;
	}

	public void setStock_prod(int stock_prod) {
		this.stock_prod = stock_prod;
	}

	public double getPrecio_prod() {
		return precio_prod;
	}

	public void setPrecio_prod(double precio_prod) {
		this.precio_prod = precio_prod;
	}

	public Almacenamiento getObjAlm() {
		return objAlm;
	}

	public void setObjAlm(Almacenamiento objAlm) {
		this.objAlm = objAlm;
	}

	public Marca getObjMarc() {
		return objMarc;
	}

	public void setObjMarc(Marca objMarc) {
		this.objMarc = objMarc;
	}

	
}
