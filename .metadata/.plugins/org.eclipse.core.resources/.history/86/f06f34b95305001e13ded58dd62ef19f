package com.inti.model;

import jakarta.persistence.Entity;
import jakarta.persistence.Id;
import jakarta.persistence.Table;
import lombok.Data;

@Entity
@Table (name = "tb_marca")

public class Marca {
	@Id
	private int id_marc;
	private String nomb_marc;
	
	public int getId_marc() {
		return id_marc;
	}
	public void setId_marc(int id_marc) {
		this.id_marc = id_marc;
	}
	public String getNomb_marc() {
		return nomb_marc;
	}
	public void setNomb_marc(String nomb_marc) {
		this.nomb_marc = nomb_marc;
	}
	
	
	
}
